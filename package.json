{
  "name": "cortexyoung",
  "version": "2.1.5",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp src/*.js dist/ 2>/dev/null || true",
    "dev": "ts-node src/index.ts",
    "demo": "ts-node src/index.ts",
    "demo:reindex": "ts-node src/index.ts --reindex",
    "demo:full": "ts-node src/index.ts --full",
    "server": "ts-node src/server.ts",
    "server:rebuild": "FORCE_REBUILD=true ts-node src/server.ts",
    "start": "node dist/server.js",
    "start:rebuild": "FORCE_REBUILD=true npm start",
    "start:full": "INDEX_MODE=full npm start",
    "start:incremental": "INDEX_MODE=incremental npm start",
    "start:cloudflare": "EMBEDDER_TYPE=cloudflare npm start",
    "clean": "rm -rf dist",
    "cache:stats": "node scripts/manage-embeddings.js stats",
    "cache:clear": "node scripts/manage-embeddings.js clear",
    "cache:validate": "node scripts/manage-embeddings.js validate",
    "cache:backup": "node scripts/manage-embeddings.js backup",
    "cache:sync-global": "node scripts/manage-embeddings.js sync-to-global",
    "cache:sync-local": "node scripts/manage-embeddings.js sync-to-local",
    "cache:info": "node scripts/manage-embeddings.js info",
    "cache:clear-all": "npm run cache:clear && node scripts/manage-relationships.js clear",
    "relationships:stats": "node scripts/manage-relationships.js stats",
    "relationships:clear": "node scripts/manage-relationships.js clear",
    "relationships:sync-global": "node scripts/manage-relationships.js sync-to-global",
    "relationships:sync-local": "node scripts/manage-relationships.js sync-to-local",
    "relationships:info": "node scripts/manage-relationships.js info",
    "storage:status": "node scripts/manage-unified-storage.js status",
    "storage:stats": "node scripts/manage-unified-storage.js stats",
    "storage:validate": "node scripts/manage-unified-storage.js validate",
    "storage:sync": "node scripts/manage-unified-storage.js sync",
    "storage:clear": "node scripts/manage-unified-storage.js clear",
    "rebuild": "npm run cache:clear && npm run server:rebuild",
    "health": "ts-node src/health-check-cli.ts . check",
    "health:check": "ts-node src/health-check-cli.ts . should-rebuild",
    "test:mcp": "node test-mcp.js",
    "test:worker-pool": "npm run build && node test-worker-pool.js",
    "test:process-pool": "npm run build && node test-process-pool.js",
    "benchmark": "ts-node src/benchmark-cli.ts",
    "benchmark:startup": "ts-node src/benchmark-cli.ts --category startup",
    "benchmark:search": "ts-node src/benchmark-cli.ts --category search",
    "benchmark:storage": "ts-node src/benchmark-cli.ts --category storage",
    "benchmark:full": "ts-node src/benchmark-cli.ts --category all --iterations 3",
    "validate:performance": "node validate-performance.js",
    "test:performance": "npm run validate:performance",
    "benchmark:adaptive": "node benchmark-adaptive-pool.js",
    "benchmark:quick": "node quick-benchmark.js",
    "test:adaptive-features": "node test-adaptive-features.js",
    "test:adaptive-pool": "npm run test:adaptive-features",
    "test:cleanup": "node test-direct-cleanup.js",
    "test:orphan-prevention": "node test-orphan-prevention.js",
    "test:final-cleanup": "node final-cleanup-test.js",
    "test:cpu-memory": "node test-cpu-memory-scaling.js",
    "test:signal-cascade": "node test-signal-cascade.js",
    "test:auto-sync-staleness": "npm run build && node test-auto-sync-staleness.js",
    "shutdown": "node scripts/shutdown.js",
    "startup": "node scripts/startup.js",
    "health": "node scripts/health-check.js",
    "status": "node scripts/check-server-status.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/semver": "^7.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "async-mutex": "^0.5.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fastembed": "^1.14.4",
    "fastq": "^1.19.1",
    "semver": "^7.7.2",
    "simple-git": "^3.28.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-typescript": "^0.23.2",
    "zod": "^3.25.76"
  }
}
